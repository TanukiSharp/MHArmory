<Window x:Class="MHArmory.LoadoutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MHArmory"
        mc:Ignorable="d"
        Title="Loadout" Width="800" Height="450" WindowStartupLocation="CenterOwner" Background="WhiteSmoke">
    <ContentControl Content="{Binding Mode=OneTime}" ContentTemplate="{StaticResource LoadoutSelectorView}" Focusable="False"/>
</Window>
